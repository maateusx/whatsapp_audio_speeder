{
  "manifest_version": 2,
  "name": "Whatsapp Audio Speeder",
  "description": "Change speed rate of a audio or video from Whatsapp Web or anywhere.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
   "activeTab"
  ]
}